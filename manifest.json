{
    "manifest_version": 2,
    "name": "HTTP Switchboard",
    "version": "0.1.4",
    "description": "Point & click to forbid/allow any class of requests made by your browser. Use it to block scripts, iframes, ads, facebook, etc.",
    "browser_action": {
        "default_icon": {
            "19": "img/scripthq19.png",
            "38": "img/scripthq38.png"
        },
        "default_title": "HTTP Switchboard",
        "default_popup": "popup.html"
    },
    "author": "Raymond Hill",
    "background": {
        "scripts": ["js/jquery-2.min.js","background.js"]
    },
    "homepage_url": "https://github.com/gorhill/httpswitchboard",
    "minimum_chrome_version": "28.0",
    "options_page": "options.html",
    "permissions": [
        "storage",
        "tabs",
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
